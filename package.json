{
  "name": "pdfgen",
  "productName": "pdfgen",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": "dst/index.js",
  "scripts": {
    "start": "tsc && electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [],
  "author": {
    "name": "Hattmo",
    "email": "matthew@hattmo.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "pdfgen"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.56",
    "@electron-forge/maker-deb": "^6.0.0-beta.56",
    "@electron-forge/maker-rpm": "^6.0.0-beta.56",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.56",
    "@electron-forge/maker-zip": "^6.0.0-beta.56",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "electron": "12.0.9",
    "eslint": "^7.27.0",
    "eslint-plugin-import": "^2.23.3",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@hattmo/pdfgenfe": "^0.1.1",
    "@types/node": "^15.6.0",
    "@types/node-pdftk": "^2.1.4",
    "@types/uuid": "^8.3.0",
    "csv-parser": "^3.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "node-pdftk": "^2.1.3",
    "uuid": "^8.3.2"
  }
